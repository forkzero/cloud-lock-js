  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.1",
    "@types/sinon": "^7.5.0",
    "@types/sinon-chai": "^3.2.3",
    "chai": "^4.2.0",
    "growl": "^1.10.5",
    "json-loader": "^0.5.7",
